{
  "name": "Pomodoro",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "startTime": {
      "type": "date",
      "required": true
    },
    "duration": {
      "type": "number"
    },
    "interrupts": {
      "type": "number"
    },
    "status": {
      "type": "string",
      "index": true
    }
  },
  "validations": [],
  "relations": {
    "member": {
      "type": "belongsTo",
      "model": "Member",
      "foreignKey": "memberId"
    },
    "task": {
      "type": "hasOne",
      "model": "Task",
      "foreignKey": "taskId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "*"
    }
  ],
  "methods": []
}
